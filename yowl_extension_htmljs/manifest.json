{
  "manifest_version": 3,
  "name": "Yowl extension",
  "version": "1.0",

  "description": "Discover our new Yowl Extension ",

  "icons": {
    "48": "icons/mainlogo.png",
    "96": "icons/mainlogo.png"
  },


  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],

  "action": {
    "default_icon": "icons/mainlogo.png",
    "default_title": "Yowl Extension",
    "default_popup": "popup/landingpage.html"
  }

}
